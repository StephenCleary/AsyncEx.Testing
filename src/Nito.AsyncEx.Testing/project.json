{
  "version": "1.0.0",
  "authors": ["Stephen Cleary"],
  "description": "Helpful types for writing asynchronous unit tests.",
  "buildOptions": {
    "warningsAsErrors": true,
    "xmlDoc": true
  },
  "packOptions": {
    "tags": [ "async", "unit", "test" ],
    "projectUrl": "https://github.com/StephenCleary/AsyncEx.Testing",
    "licenseUrl": "https://raw.githubusercontent.com/StephenCleary/AsyncEx.Testing/master/LICENSE",
    "repository": {
      "type": "git",
      "url": "https://github.com/StephenCleary/AsyncEx.Testing.git"
    }
  },
  "frameworks": {
    "netstandard1.0": {
      "dependencies": {
        "System.Runtime": "4.1.0",
        "System.Threading.Tasks": "4.0.11"
      }
    },
    ".NETPortable,Version=v4.5,Profile=Profile259": {
      "frameworkAssemblies": {
        "System.Runtime": { "type": "build" },
        "System.Threading.Tasks": { "type": "build" }
      }
    }
  }
}
